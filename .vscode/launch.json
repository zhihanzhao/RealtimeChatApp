  {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Angular Debug (Chrome)",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "trace": true,
        "sourceMapPathOverrides": {
          "webpack:///./*": "${webRoot}/*",
          "webpack:///src/*": "${webRoot}/src/*"
        }
      }
    ]
  }
  

  